{
  "name": "server-base",
  "version": "0.0.1",
  "description": "Base for Express server",
  "main": "index.js",
  "scripts": {
    "test": "mocha --watch --compilers js:babel-core/register ./test/**/*.spec.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "dev": "npm run watch",
    "open:src": "babel-node server",
    "lint": "node_modules/.bin/esw webpack.config.* src server",
    "lint:watch": "npm run lint -- --watch",
    "watch": "nodemon --ignore *.spec.js server --ignore server/localDatabase/* --exec babel-node",
    "build": "rm -rf dist/ && babel server -d dist",
    "serve": "node dist"
  },
  "author": "Sami Nieminen",
  "license": "ISC",
  "dependencies": {
    "apicache": "^1.0.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "crypto": "0.0.3",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "helmet": "^3.6.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "pg-promise": "^6.3.5",
    "sequelize": "^4.4.2"
  },
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "6.8.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "2.1.11",
    "expect": "^1.20.2",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.5.1",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "sass-loader": "^6.0.3",
